<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#c5eefd"
    tools:context="com.example.ozeki.fishing5.FishingActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="fill_parent"
        android:layout_height="200dp"
        android:scaleType="fitEnd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/background" />

    <Button
        android:id="@+id/start_fishing_button"
        android:layout_width="158dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:layout_marginEnd="217dp"
        android:layout_marginStart="217dp"
        android:background="@drawable/shape_rounded_corners_10dp"
        android:text="@string/start_fishing"
        android:textColor="@android:color/white"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/target_location_view"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:visibility="visible" />

        <ImageView
            android:id="@+id/target_location_button"
            android:layout_width="65dp"
            android:layout_height="75dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/frameLayout2"
            android:layout_marginStart="114dp"
            android:layout_marginTop="86dp"
            android:background="@null"
            android:scaleType="fitStart"
            android:src="@drawable/location" />

        <ImageView
            android:id="@+id/fishing_cat"
            android:layout_width="150dp"
            android:layout_height="150dp"
            tools:layout_editor_absoluteX="433dp"
            tools:layout_editor_absoluteY="198dp" />

        <ImageView
            android:id="@+id/light"
            android:layout_width="50dp"
            android:layout_height="50dp"
            tools:layout_editor_absoluteX="433dp"
            tools:layout_editor_absoluteY="198dp" />

        <ImageView
            android:id="@+id/turizao"
            android:layout_width="50dp"
            android:layout_height="50dp"
            tools:layout_editor_absoluteX="433dp"
            tools:layout_editor_absoluteY="198dp" />

    </FrameLayout>

</android.support.constraint.ConstraintLayout>
